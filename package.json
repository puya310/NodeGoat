  "name": "consumer-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "node ./scripts/migrate",
    "preinstall": "rm -f ./node_modules/@truecar/theming",
    "postinstall": "if test \"$SKIP_POSTINSTALL\" != \"true\"; then npm run build:packages; fi && patch-package",
    "build:nextjs-consumer-server": "NODE_ENV=production webpack --config ./webpack/webpack-config-nextjs-server.js",
    "build:nextjs-dealer-server": "NODE_ENV=production webpack --config ./webpack/webpack-config-dealer-nextjs-server.js",
    "build:packages": "rm -rf packages/graphql-codegen-add-ids/dist && cd packages/graphql-codegen-add-ids && tsc -d",
    "build:next-server": "cd apps/consumer-next/server && node ./buildServer.js",
    "start": "npm run dealer:start",
    "dev:before": "check-dependencies && node ./scripts/checkExtensions && node ./scripts/checkVersions && concurrently -r \"npm run postinstall\" \"npm run graphql:introspection\" && npm run graphql:generate",
    "consumer:dev": "npm run migrate && npm run dev:before && npm run build:nextjs-consumer-server && concurrently --kill-others -r \"node ./scripts/codegenWatch.js --all\" \"cd apps/consumer-next && NODE_ENV=development node ./server/build/server.js\"",
    "consumer:build": "cd apps/consumer-next && NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "consumer:build:analyze": "cd apps/consumer-next && NODE_OPTIONS=\"--max-old-space-size=4096\" ANALYZE_BUNDLE=true next build",
    "consumer:start": "cd apps/consumer-next && NODE_ENV=production node ./server/build/server.js",
    "start-both-servers": "NODE_ENV=production pm2-runtime start nextEcosystem.config.js",
    "dealer:dev": "npm run migrate && npm run dev:before && npm run build:nextjs-dealer-server && concurrently --kill-others -r \"node ./scripts/codegenWatch.js --all\" \"cd apps/dealer-next && NODE_ENV=development node ./server/build/server.js\"",
    "dealer:build": "cd apps/dealer-next && NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "dealer:start": "cd apps/dealer-next && NODE_ENV=production node ./server/build/server.js",
    "start:dev": "babel-node ./server/start.js",
    "start:dev-server": "node --max-old-space-size=2048 ./server/devServer",
    "test": "check-dependencies && jest",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,json,scss,md}\"",
    "prettier:check": "prettier --check '**/*.{js,ts,tsx,json,scss,md}'",
    "prettier:check-version": "./scripts/checkPrettierVersion",
    "prettier-staged": "lint-staged",
    "compress-images": "./bin/compress-images",
    "checks": "babel-node ./bin/lint/check-for-unlocked-deps.js",
    "icons:generate": "babel-node ./scripts/generateIcons.js",
    "lint": "npm run checks && npm run typescript && npm run eslint && npm run stylelint",
    "eslint": "LINT_ERRORS=true eslint apps docs src libs packages webpack server cypress testcafe --ext=.js,.ts,.tsx",
    "eslint:test": "jest --testPathPattern packages/eslint-plugin-truecar",
    "lint:fix": "npm run eslint -- --fix; node scripts/removeUnusedTsExpectErrors.js",
    "graphql:clean": "find ./src -name \"*.graphql.tsx\" -type f -delete",
    "css:generate": "node ./scripts/codegenRun --css",
    "runtime:generate": "node ./scripts/codegenRun --all",
    "runtime:generate:watch": "node ./scripts/codegenWatch --all",
    "graphql:generate": "node ./scripts/codegenRun --graphql",
    "graphql:generate:watch": "node ./scripts/deprecatedScript.js \"graphql:generate:watch\" \"runtime:generate:watch\"",
    "graphql:introspection": "graphql-codegen -r dotenv/config -c ./codegenIntrospection.js",
    "graphql:schema": "graphql-codegen -r dotenv/config -c ./codegenSchema.js",
    "partners:generate": "node ./scripts/generatePartnerList",
    "stylelint": "stylelint 'packages/battery-pack/**/*.scss'",
    "typescript": "node --max-old-space-size=4096 ./node_modules/.bin/tsc --project ./tsconfig.json && node --max-old-space-size=4096 ./node_modules/.bin/tsc --project ./cypress/tsconfig.json && node --max-old-space-size=4096 ./node_modules/.bin/tsc --project ./performance/tsconfig.json",
    "ci": "npm run lint && npm run test",
    "on-docker": "docker run -p=8888:8888 --name=consumer-frontend --rm -it -e ABP_BACKEND_APP_HOST=<ABP_BACKEND_SPACEPODS_URI> consumer-frontend",
    "cy:open": "npm run migrate && npm run dev:before && TZ=UTC cypress open",
    "cy:run:record": "node ./bin/cypress-run",
    "cy:stability": "node ./bin/cypress-stability",
    "testcafe": "testcafe chrome testcafe/tests/ --skip-js-errors -s -r spec,xunit:tests/report.xml",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook  -c .storybook -o build/docs",
    "testcafe-sauce-edge": "testcafe 'saucelabs:MicrosoftEdge@latest:Windows 10,saucelabs:iPhone 11 Simulator@13.4' testcafe/tests/ -q -c 1 --skip-js-errors --browser-init-timeout 420000",
    "update-version": "node ./scripts/updateVersion.js",
    "find-unused": "ts-prune --ignore 'apps/consumer-next/pages|apps/dealer-next/pages|docs/|packages/|libs/' | grep -Ev 'index|Fragment'"
  },
  "pre-commit": [
    "prettier-staged"
  ],
  "author": "test",
  "license": "ISC",
  "private": true,
  "overrides": {
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "@types/react": "18.0.28"
  }
}
